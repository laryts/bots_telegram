{
  "$schema": "https://railway.app/railway.schema.json",
  "$comment": "This project is in the diindiin/ subdirectory",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "cd diindiin && node dist/index.js",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}

